	author = {Stuart-Smith, Rick D. and Edgar, Graham J. and Clausius, Ella and Oh, Elizabeth S. and Barrett, Neville S. and Emslie, Michael J. and Bates, Amanda E. and Bax, Nic and Brock, Daniel and Cooper, Antonia and Davis, Tom R. and Day, Paul B. and Dunic, Jillian C. and Green, Andrew and Hasweera, Norfaizny and Hicks, Jamie and Holmes, Thomas H. and Jones, Ben and Jordan, Alan and Knott, Nathan and Larkin, Meryl F. and Ling, Scott D. and Mooney, Peter and Pocklington, Jacqueline B. and Seroussi, Yanir and Shaw, Ian and Shields, Derek and Smith, Margo and Soler, German A. and Stuart-Smith, Jemina and Turak, Emre and Turnbull, John W. and Mellin, Camille},
	month = sep,
