	pages = {391--408},
	file = {Labra et al (2015) Dynamic relationships between body size, species richness, abundance, and.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Labra et al (2015) Dynamic relationships between body size, species richness, abundance, and.pdf:application/pdf},
