	pages = {177--188},
	file = {Daan et al (2005) Changes in the North Sea fish community.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Daan et al (2005) Changes in the North Sea fish community.pdf:application/pdf},
