	year = {2010},
	pages = {1361--1382},
	file = {Datta et al (2010) A Jump-Growth Model for Predator–Prey Dynamics.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Datta et al (2010) A Jump-Growth Model for Predator–Prey Dynamics.pdf:application/pdf},
