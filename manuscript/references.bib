	file = {Mittelbach et al (2007) Evolution and the latitudinal diversity gradient.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Mittelbach et al (2007) Evolution and the latitudinal diversity gradient.pdf:application/pdf},
